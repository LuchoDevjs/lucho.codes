---
import {
  FaCss3Alt,
  FaNode,
  FaReact,
  FaGit,
  FaGithub,
  FaJsSquare,
  FaSass,
} from "react-icons/fa/index.js";
import { SiRedux, SiMongodb, SiExpress } from "react-icons/si/index.js";
---

<h1 class="skills__title" id="skills">Skills</h1>
<section class="skills">
  <section class="skills__list">
    <div class="skills__css">
      <FaCss3Alt size={75} />
      <p>Css</p>
    </div>
    <div class="skills__js">
      <FaJsSquare size={75} />
      <p>Javascript</p>
    </div>
    <div class="skills__nodejs">
      <FaNode size={75} />
      <p>Node JS</p>
    </div>
    <div class="skills__mongodb">
      <SiMongodb size={75} />
      <p>Mongo DB</p>
    </div>
    <div class="skills__express">
      <SiExpress size={75} />
      <p>Express</p>
    </div>
    <div class="skills__reactjs">
      <FaReact size={75} />
      <p>React JS</p>
    </div>
    <div class="skills__redux">
      <SiRedux size={75} />
      <p>Redux</p>
    </div>
    <div class="skills__sass">
      <FaSass size={75} />
      <p>Sass</p>
    </div>
    <div class="skills__git">
      <FaGit size={75} />
      <p>Git</p>
    </div>
    <div class="skills__github">
      <FaGithub size={75} />
      <p>GitHub</p>
    </div>
  </section>
</section>
